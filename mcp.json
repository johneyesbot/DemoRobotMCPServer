{
  "mcpServers": {
    "EyesBot": {
      "command": "/opt/homebrew/bin/uv",
      "args": [
        "run",
        "--with",
        "mcp[cli]",
        "--with",
        "responses",
        "mcp",
        "run",
        "/Users/[your user here!!]/Documents/Code/AI/RobotMCP/MCPServer/main.py"
      ],
      "env": {
        "ROBOT_BASE_URL": "[your robot's IP address here!!]:[your robot's port server port number here!!]",
        "SECURE_URL": "False"
      }
    }
  }
}
